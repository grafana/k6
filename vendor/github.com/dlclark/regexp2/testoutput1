  #comment
   b $ /x
    aaaab
 0: aaaab
    
/ ^ (?> a + ) b $ /x
    aaaab 
 0: aaaab

/ ^ ( a + ) + \w $ /x
    aaaab 
 0: aaaab
 1: aaaa

